{
  "name": "@asaddu/pdfme-pdf-lib",
  "version": "0.0.0",
  "sideEffects": false,
  "author": "hand-dot",
  "license": "MIT",
  "keywords": [
    "pdf-lib",
    "pdf",
    "document",
    "create",
    "modify",
    "creation",
    "modification",
    "edit",
    "editing",
    "typescript",
    "javascript",
    "library"
  ],
  "description": "Create and modify PDF files with JavaScript",
  "homepage": "https://pdfme.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:Asaddu/pdfme.git"
  },
  "bugs": {
    "url": "https://github.com/Asaddu/pdfme/issues"
  },
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "eslint --ext .ts src --config eslint.config.mjs",
    "test": "vitest run",
    "prune": "ts-prune src",
    "prettier": "prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "@pdf-lib/standard-fonts": "^1.0.0",
    "@pdf-lib/upng": "^1.0.1",
    "color": "^5.0.3",
    "node-html-better-parser": "^1.5.8",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@pdf-lib/fontkit": "^1.1.0",
    "@types/color": "^4.2.0",
    "@types/node": "^24.10.1",
    "@types/pako": "^2.0.4"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  }
}
